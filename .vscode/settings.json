{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/.vscode": true,
        "**/*.map": true,
        "tests/__pycache__": true
    },
    "hide-files.files": [
        "tests/__pycache__",
        "api/__pycache__"
    ],
    "python.analysis.extraPaths": [
        ".",
        "./api/db"
    ]
}